getgenv().ScriptAccessURL = "https://raw.githubusercontent.com/dawwiqingyi/daweiqingyi/refs/heads/main/pro.lua"

-- 通过网络加载并运行脚本
local success, errorMsg = pcall(function()
    local scriptContent = game:HttpGet("https://raw.githubusercontent.com/dawwiqingyi/daweiqingyi/refs/heads/main/pro.lua")
    loadstring(scriptContent)()
end)

if not success then
    warn("加载脚本失败: " .. errorMsg)
end
